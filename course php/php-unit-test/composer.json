{
    "name": "daudhidayatramadhan/belajar-php-unit-test",
    "description": "belajar PHP Unit test",
    "type": "project",
    "authors": [
        {
            "name": "DaudHidayatRamadhn",
            "email": "daud28ramadhan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Daudhidayatramadhan\\BelajarPhpUnitTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daudhidayatramadhan\\BelajarPhpUnitTest\\": "tests"
        }
    },
    "scripts": {
        "test" : "phpunit --configuration phpunit.xml",
        "test:product" : "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter" : "phpunit --configuration phpunit.xml --testsuite counter"
    }
}
